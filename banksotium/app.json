{
  "endpoints": {
    "/heartbeat":{
      "get": {
        "js_module": "app.js",
        "js_function": "heartbeat",
        "forwarding_required": "always",
        "authn_policies": [],
        "mode": "readonly",
        "openapi": {}
      }
    },
    "/balances/PFI/{pfi}":{
      "get": {
        "js_module": "endpoints/balances.js",
        "js_function": "getpfibalance",
        "forwarding_required": "never",
        "authn_policies": ["user_cert"],
        "mode": "readonly",
        "openapi": {}
      }
    },
    "/pledge":{
      "post": {
        "js_module": "endpoints/cbonlyop.js",
        "js_function": "pledge",
        "forwarding_required": "always",
        "authn_policies": ["user_cert"],
        "mode": "readwrite",
        "openapi": {}
      }
    },
    "/register/thyself":{
      "post":{
        "js_module": "endpoints/cbonlyop.js",
        "js_function": "registerthyself",
        "forwarding_required": "always",
        "authn_policies": ["member_cert"],
        "mode": "readwrite",
        "openapi": {
          "responses": {
            "200": {
              "description": "Ok"
            }
          },
          "security": [],
          "parameters": [],
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "user_id": {
                      "type": "string"
                    }
                  },
                  "type": "object"
                }
              }
            }
          }
        }
      }
    }
  }
}
